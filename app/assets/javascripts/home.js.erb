var tilejson = {
  tilejson: '1.0.0',
  scheme: 'xyz',
  tiles: ['http://a.tiles.mapbox.com/v3/onlyjsmith.wildspot-map/{z}/{x}/{y}.png']
};

var url = 'http://a.tiles.mapbox.com/v3/onlyjsmith.wildspot-map.jsonp';
// Alias com.modestmaps to mm. This isn't necessary -
// just nice for shorter code.
var mm = com.modestmaps;

wax.tilejson(url, 
  function(tilejson) {
    var m = new mm.Map('map', new wax.mm.connector(tilejson));

    wax.mm.zoomer(m, tilejson).appendTo(m.parent);
    m.setCenterZoom(new mm.Location(-12.6, 26), 5);
  });
